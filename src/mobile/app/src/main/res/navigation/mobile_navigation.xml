<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/TopFragment">

    <fragment
        android:id="@+id/TopFragment"
        android:name="com.example.judgesgandroid.ui.TopFragment"
        android:label="top_fragment"
        tools:layout="@layout/fragment_top">
        <action
            android:id="@+id/action_TopFragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>
    <fragment
        android:id="@+id/startFragment"
        android:name="com.example.judgesgandroid.ui.StartFragment"
        android:label="StartFragment"
        tools:layout="@layout/fragment_start" />

</navigation>